#include<stdio.h>
int main()
{
	int n;
    printf("enter the size of the array:")
    scanf("%d",&n);
	int a[n-1];
	printf("enter the elements of the array:")
	int sum = 0;
	for(int i=0;i<n-1;i++)
	{
		scanf("%d",&a[i]);
		sum += a[i];
	}
	int total_sum  = (n*(n=1))/2;
	printf("the missing elements is %d",total_sum-sum);
	return 0;
}
